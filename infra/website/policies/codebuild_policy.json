{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Action": [
         "s3:PutObject",
         "s3:GetObject",
         "s3:GetObjectVersion",
         "s3:GetBucketVersioning"
        ],
        "Resource": "*",
        "Effect": "Allow"
      },
      {
        "Action": [
            "s3:*"
        ],
        "Resource": [
          "${aws_s3_bucket_arn}",
          "${aws_s3_bucket_arn/*"
        ],
        "Effect": "Allow"
      },
      {
        "Action": [
          "logs:CreateLogGroup",
          "logs:CreateLogStream",
          "logs:PutLogEvents"
        ],
        "Resource": [
          "*"
        ],
        "Effect": "Allow"
      },
      {
        "Action": [
          "cloudfront:CreateInvalidation"
        ],
        "Resource": "*",
        "Effect": "Allow"
      }
    ]
  }